{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "VITE_AWS_REGION": "@vite_aws_region",
    "VITE_AWS_ACCESS_KEY_ID": "@vite_aws_access_key_id",
    "VITE_AWS_SECRET_ACCESS_KEY": "@vite_aws_secret_access_key",
    "VITE_BEDROCK_MODEL_ID": "@vite_bedrock_model_id",
    "VITE_GOOGLE_API_KEY": "@vite_google_api_key",
    "VITE_GOOGLE_CLIENT_ID": "@vite_google_client_id",
    "VITE_API_URL": "@vite_api_url",
    "VITE_USE_MOCK_DATA": "false",
    "VITE_ENABLE_AWS_BEDROCK": "true",
    "VITE_PRODUCTION_MODE": "true",
    "VITE_ENVIRONMENT": "production"
  },
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}